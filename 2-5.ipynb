   "source": [
    "from sympy import symbols, diff, solve\n",
    "\n",
    "a = symbols('a')\n",
    "x1, y1 = 4, 1\n",
    "x2, y2 = 6, 4\n",
    "\n",
    "f_a = (y1 - a*x1)**2 + (y2 - a*x2)**2\n",
    "\n",
    "f_prime = diff(f_a, a)\n",
    "print(f_prime)\n",
    "optimal_a = solve(f_prime, a)\n",
    "optimal_a"
   ]
  }
